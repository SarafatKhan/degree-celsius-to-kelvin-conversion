#include <stdio.h>

int main() 
{
    int i;
    for(i = 1; i<10; i++){
        
        int num1;
        
        printf("Enter Any Number\n");
        scanf("%d", &num1);
        int num2 = num1;
        
        int lastNum = num1*10;
        for(num1; num1<=lastNum; num1 = num1 + num2){
            
        printf("%d\n", num1);
    
        }
        
        
    }
    return 0;
}
